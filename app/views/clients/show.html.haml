- provide(:title, "#{@client.combine_names} profile")

.signed-in-content  
  - c = @client
  .details-client.scrollable
    .details-header-client
      %h1 #{c.combine_names}
    .details-content
      %h2 Project details
      %ul
        %li 
          %label Client name
          %span #{c.combine_names}
        %li
          %label Project budget
          %span $#{c.budget}
        %li
          %label Description
          %span #{c.descr}
    






    