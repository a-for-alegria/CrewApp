- provide(:title, "#{@crew.combine_names} profile")

.signed-in-content
  - c = @crew
  .profile-crew.scrollable
    .details-header-crew
      %h1 #{c.combine_names}
    .profile-crew-info
      %h2 Personal info
      %ul
        %li
          %label Fullname
          %span #{c.combine_names}
        %li 
          %label Date of birth
          %span 21.12.1989
        %li
          %label Specialization
          %span #{c.spec}
        -if c.bank_card
          %li 
            %label Bank card
            %span #{c.bank_card}
        %li
          %label Rate/month 
          %span $#{c.rate}   
        %li
          %label Project
          %span #{c.client_project}

          


    
